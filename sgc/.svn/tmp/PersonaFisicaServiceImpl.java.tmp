package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.PersonaFisicaDao;
import com.soma.transmisor.model.PersonaFisica;
import com.soma.transmisor.service.PersonaFisicaService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Repository("personaFisicaService")
@Transactional
public class PersonaFisicaServiceImpl implements PersonaFisicaService {

    @Autowired
    PersonaFisicaDao personaFisicaDao;
    private boolean estadoMetodo = false;

    @Override
    public boolean save(PersonaFisica personaFisica) {
        personaFisicaDao.save(personaFisica);
        return estadoMetodo = true;
    }

    @Override
    public boolean update(PersonaFisica personaFisica) {
        personaFisicaDao.update(personaFisica);
        return estadoMetodo = true;
    }

    @Override
    public PersonaFisica buscaCliente(int clienteid) {
        return personaFisicaDao.buscaCliente(clienteid);
    }

    @Override
    public PersonaFisica buscaPersona(String filtro, int parametro) {
        return personaFisicaDao.buscaPersona(filtro, parametro);
    }

    @Override
    public PersonaFisica buscaNoidentificacion(String noIdentificacion) {
        return personaFisicaDao.buscaNoidentificacion(noIdentificacion);
    }

    @Override
    public List<PersonaFisica> showFisica() {
        return personaFisicaDao.showFisica();
    }

}
