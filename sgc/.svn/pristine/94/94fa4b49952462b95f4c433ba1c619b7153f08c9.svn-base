package com.soma.transmisor.model;

import java.io.Serializable;
import java.util.Date;
import javax.persistence.CascadeType;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.ManyToOne;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;
import org.hibernate.annotations.ForeignKey;

/**
 *
 * @author JASMIN-SOMA
 */
@Entity
@Table(name="perfil_cliente")
public class PerfilCliente implements Serializable{
    @Id
   @GeneratedValue(strategy = GenerationType.IDENTITY)
   @Column(name = "perfilid")
   private Integer perfilid;
    @Column(name = "limite")
   private float limite;
    @Temporal(TemporalType.DATE) 
   @Column(name = "fecha")
   private Date fecha;
    @ManyToOne(cascade = CascadeType.ALL)
    @ForeignKey(name = "clienteid_fk")
   private Cliente clienteid;

    public Integer getPerfilid() {
        return perfilid;
    }

    public void setPerfilid(Integer perfilid) {
        this.perfilid = perfilid;
    }

    public float getLimite() {
        return limite;
    }

    public void setLimite(float limite) {
        this.limite = limite;
    }

    public Date getFecha() {
        return fecha;
    }

    public void setFecha(Date fecha) {
        this.fecha = fecha;
    }

    public Cliente getClienteid() {
        return clienteid;
    }

    public void setClienteid(Cliente clienteid) {
        this.clienteid = clienteid;
    }

    @Override
    public String toString() {
        return "PerfilCliente{" + "perfilid=" + perfilid + ", limite=" + limite + ", fecha=" + fecha + ", clienteid=" + clienteid + '}';
    }
    
    
}
