package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.TipoIdentidadDao;
import com.soma.transmisor.model.TipoIdentidad;
import com.soma.transmisor.service.TiposIdentidadService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Repository("tipoIdentidadService")
@Transactional
public class TipoIdentidadServiceImpl implements TiposIdentidadService{
    @Autowired
    TipoIdentidadDao tipoIdnetidadDao;
    
    @Override
    public TipoIdentidad buscaTipoIdentidad(int identificacionid) {
        return tipoIdnetidadDao.buscaTipoIdentidad(identificacionid);
    }

    @Override
    public List<TipoIdentidad> showTipoIdentidad() {
        return tipoIdnetidadDao.showTipoIdentidad();
    }
    
}
