<%@include file="index.jsp" %>
<br />
<br />
<br />
<div class="container">   
    <div class="panel-group">
        <div class="panel panel-primary">
            <div class="panel-heading">Prevención de Lavado de Dinero</div>
            <div id="contenidoPantalla" class="panel-body">        
                <div class="row">
                    <label class="control-label col-lg-2" for="tipoOperacion">Tipo de persona: </label>
                                <div class="col-sm-2">
                                    <!-- consulta catalogo T. Operacion -->
                                    <SELECT id="tipoPersona" class="form-control">
                                        <option value="0">Seleccionar </option>
                                        <c:forEach items="" var="tipo">
                                            <OPTION value="1">moral</OPTION>                                            
                                            
                                            </c:forEach>
                                    </SELECT>
                                </div>
                     <div class="form-group">
                            <label  class="control-label col-sm-2" >Fecha inicio</label>
                            <div class="col-sm-2">
                                <input type="date" class="form-control" id="fechIni">
                            </div>                                 

                            <label  class="control-label col-sm-2">Fecha Termino:</label>
                            <div class="col-sm-2">
                                <input type="date" class="form-control" id="fechTer" >
                            </div>
                        </div>
                    <div class="form-group">
                        <label  class="control-label col-sm-2">Monto: </label>
                            <div class="col-sm-2">
                                <input type="number" class="form-control" id="montoPld" placeholder="Ingresa la cantidad a buscar">
                            </div>  
                        <label  class="control-label col-sm-2" id="dolares">Dolares: $5000 </label>
                            
                    </div>
                    <!-- Pase de clientes-->
                    <div  class="control-label col-lg-2" style="width: 100%; display: table;">
                        <div style="display: table-row">
                            <div style="width: 315px; display: table-cell;">
                                <label>Lista de clientes: </label><br>
                                <select multiple name="tramiteClientes[]" id="tramiteClientes" style="width: 300px; height: 200px;">
                                    <c:forEach items="${lPba}" var="pba"> 
                                        <option value="${pba.pbaid}">${pba.personaid.nombre}  ${pba.personaid.apaterno} ${pba.personaid.amaterno}</option> 
                                    </c:forEach> -->
                                    <!--   <option value="0">Jasmin </option> -->

                                </select>                
                            </div>
                            <div style="width: 315px; display: table-cell;">
                                <br><br><br>
                                <a href="" id="pasar"  title="Agregar Clientes">

                                    <span class="glyphicon glyphicon-circle-arrow-right" aria-hidden="true"></span></a><br>
                                <a href="" id="quitar"  title="Mover Clientes">
                                    <span class="glyphicon glyphicon-circle-arrow-left" aria-hidden="true"></span></a><br>
                                     
                                    <a href="" id="pasarTodo"  title="Agregar Clientes">>>></a><br>
                                    <a href="" id="quitarTodo"  title="Mover Clientes"> <<<</a>
                                 
                            </div>
                            <div style="width: 315px; display: table-cell;">
                                <label>Lista de clientes Seleccionados: </label><br>
                                <select multiple name="tramiteClientesSeleccionados[]"  id="tramiteClientesSeleccionados" style="width: 300px; height: 200px;">
                                </select>                
                            </div>
                        </div>
                    </div>
                    
                </div><!-- row-->
                <button id="reportePLD" title="Generar Reporte" class="btn btn-success" onclick="generarReportePLD();">Reporte</button>
                    
            </div><!--class body -->
        </div><!-- panel-primary-->
    </div>  <!-- panel-group-->  
</div><!--class container --> 
<div class="container">
<!-- Listado de pba -->
                    <div  class="col-md-12">
                        <div class="table-responsive">
                            <table id="tablePLD" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>No.Cliente</th>                               
                                        <th>Nombres</th>
                                        <th>Apellido Paterno</th>
                                        <th>Apellido Materno</th>  
                                        <th>CURP</th>
                                        <th style="display:none">Sexo ID</th>
                                        <th>Sexo</th>
                                        

                                    </tr>
                                </thead>
                                <tbody id="bodytabla">
                                    <c:forEach items="${lPba}" var="pba">
                                        <tr>
                                            <td id="idpba">${pba.pbaid}</td>
                                            <td id="nombrePLD"></td>
                                            <td>${pba.personaid.nombre}</td>                                        
                                            <td>${pba.personaid.apaterno}</td> 
                                            <td>${pba.personaid.amaterno}</td> 
                                            <td>${pba.personaid.curp}</td> 
                                            <td style="display:none"> ${pba.personaid.sexoid.sexoid}</td> 
                                            <td> ${pba.personaid.sexoid.descripcion}</td>
                                           

                                        </tr>

                                    </c:forEach>

                                </tbody>

                            </table>

                        </div>     <!-- Table-->      
                       
                        <input type="hidden" name="${_csrf.parameterName}"
                               value="${_csrf.token}" />
                    </div><!--col-m-12 -->
</div>
</body>
</html>
