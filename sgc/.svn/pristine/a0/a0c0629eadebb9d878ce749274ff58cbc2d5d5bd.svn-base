package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.SexoDao;
import com.soma.transmisor.model.Sexo;
import com.soma.transmisor.service.SexoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JUAN LUIS-SOMA
 */
@Service("sexoService")
@Transactional
public class SexoServiceImpl implements SexoService{

    @Autowired
    SexoDao sexoDao;
    @Override
    public Sexo buscaClave(int sexoid) {
        return sexoDao.buscaClave(sexoid);
    }

    @Override
    public List<Sexo> showSexo() {
        return sexoDao.showSexo();
    }
    
}
