package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.ParentescoDao;
import com.soma.transmisor.model.Parentesco;
import com.soma.transmisor.service.ParentescoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JASMIN-SOMA
 */
@Service("parentescoService")
@Transactional
public class ParentescoServiceImpl implements ParentescoService{
@Autowired
ParentescoDao parentescoDao;
    @Override
    public Parentesco buscaId(int parentescoid) {
  return parentescoDao.buscaId(parentescoid);
    }

    @Override
    public List<Parentesco> showParentesco() {
   return parentescoDao.showParentesco();
    }
    
}
