package com.soma.transmisor.model;

import java.io.Serializable;
import java.util.Date;
import javax.persistence.CascadeType;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.OneToOne;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;
import org.hibernate.annotations.ForeignKey;

/**
 *Operaciones Internas Preocupantes
 * @author JASMIN-SOMA
 */
@Entity
@Table(name = "opreacionPreocupante")
public class OperacionPreocupante implements Serializable{
    
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    @Column(name = "oppreocupanteid")
    private Integer oppreocupanteid;
    @Temporal(TemporalType.DATE)
    @Column(name = "fecha")//fecha en que sucedio
    private Date fecha;
    @Column(name = "descripcion")//acciones que realiz√≥
    private String descripcion;
    @Column(name = "estatus")//reportado/sin reportar
    private String estatus;
    //relacion con usuario quien realiza la denuncia
    @OneToOne(cascade = CascadeType.ALL)
    @ForeignKey(name = "usuarioid_denunciante")
    private Usuario usuarioiddenunciante;
    //relacion con usuario aquien denuncian infractor
    @OneToOne(cascade = CascadeType.MERGE)
    @ForeignKey(name = "usuarioid_infractor")
    private Usuario usuarioidinfractor;

    public Integer getOppreocupanteid() {
        return oppreocupanteid;
    }

    public void setOppreocupanteid(Integer oppreocupanteid) {
        this.oppreocupanteid = oppreocupanteid;
    }

    public Date getFecha() {
        return fecha;
    }

    public void setFecha(Date fecha) {
        this.fecha = fecha;
    }
    public String getDescripcion() {
        return descripcion;
    }

    public void setDescripcion(String descripcion) {
        this.descripcion = descripcion;
    }
    public String getEstatus() {
        return estatus;
    }

    public void setEstatus(String estatus) {
        this.estatus = estatus;
    }

    public Usuario getUsuarioiddenunciante() {
        return usuarioiddenunciante;
    }

    public void setUsuarioiddenunciante(Usuario usuarioiddenunciante) {
        this.usuarioiddenunciante = usuarioiddenunciante;
    }

    public Usuario getUsuarioidinfractor() {
        return usuarioidinfractor;
    }

    public void setUsuarioidinfractor(Usuario usuarioidinfractor) {
        this.usuarioidinfractor = usuarioidinfractor;
    }

    @Override
    public String toString() {
        return "OperacionPreocupante{" + "oppreocupanteid=" + oppreocupanteid + ", fecha=" + fecha + ", descripcion=" + descripcion + ", estatus=" + estatus + ", usuarioiddenunciante=" + usuarioiddenunciante + ", usuarioidinfractor=" + usuarioidinfractor + '}';
    }

  
}
