package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.TelefonoDao;
import com.soma.transmisor.model.Telefono;
import com.soma.transmisor.service.TelefonoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JUAN LUIS-SOMA
 */
@Service("telefonoService")
@Transactional
public class TelefonoServiceImpl implements TelefonoService {

    @Autowired
    TelefonoDao telefonoDao;

    private boolean estadoMetodo = false;

    @Override
    public Telefono buscaClave(int telefonoid) {
        return telefonoDao.buscaClave(telefonoid);
    }

    @Override
    public List<Telefono> showTelefono() {
        return telefonoDao.showTelefono();
    }

    @Override
    public boolean update(Telefono telefono) {
        telefonoDao.update(telefono);
        return estadoMetodo = true;
    }

    @Override
    public Telefono telefono(String telefono) {
        return telefonoDao.telefono(telefono);
    }

}
