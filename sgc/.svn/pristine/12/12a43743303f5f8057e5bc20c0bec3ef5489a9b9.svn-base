package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.CausaRechazoDao;
import com.soma.transmisor.model.CausaRechazo;
import com.soma.transmisor.service.CausaRechazoService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Service("causaRechazoService")
@Transactional
public class CausaRechazoServiceImpl implements CausaRechazoService{

    @Autowired
    CausaRechazoDao causaRechazoDao;
    
    private boolean estadoMetodo = false;
    
    @Override
    public boolean save(CausaRechazo causaRechazo) {
        causaRechazoDao.save(causaRechazo);
        return estadoMetodo = true;
    }

    @Override
    public boolean update(CausaRechazo causaRechazo) {
        causaRechazoDao.update(causaRechazo);
        return estadoMetodo = true;
    }
    
}
