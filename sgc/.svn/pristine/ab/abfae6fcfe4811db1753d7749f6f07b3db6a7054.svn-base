package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.InstrumentoMonetarioDao;
import com.soma.transmisor.model.InstrumentoMonetario;
import com.soma.transmisor.service.InstrumentoMonetarioService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Service("instrumentoMonetarioService")
@Transactional
public class InstrumentoMonetarioServiceImpl implements InstrumentoMonetarioService{

    @Autowired
    InstrumentoMonetarioDao instrumentoMonetarioDao;
    @Override
    public InstrumentoMonetario buscaClave(int instrumentoid) {
       return instrumentoMonetarioDao.buscaClave(instrumentoid);
    }

    @Override
    public List<InstrumentoMonetario> showInstrumento() {
       return instrumentoMonetarioDao.showInstrumento();
    }
    
}
