package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.ActividadEconomicaDao;
import com.soma.transmisor.model.ActividadEco;
import com.soma.transmisor.service.ActividadEconomicaService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */

@Repository("actividadEconomicaService")
@Transactional
public class ActividadEconomicaServiceImpl implements ActividadEconomicaService{
    
    @Autowired
    ActividadEconomicaDao actividadEconomicaDao;
    
    @Override
    public ActividadEco buscaActividadEconomica(int actividadid) {
        return actividadEconomicaDao.buscaActividadEconomica(actividadid);
    }

    @Override
    public List<ActividadEco> showActividadEconomica() {
        return actividadEconomicaDao.showActividadEconomica();
    }
    
}
