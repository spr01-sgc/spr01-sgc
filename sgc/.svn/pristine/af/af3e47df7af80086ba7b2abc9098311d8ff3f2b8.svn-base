<label class="control-label col-lg-1"></label>
<div id="divpersonafisica" style="display: none">
    <div class="panel panel-group">
        <div class="form-group div-azul">
            <label class="control-label col-lg-1"></label>
            <label class="control-label"> <h5>BUSCAR PERSONA POR:</h5></label>
        </div>
        <div id="divBuscaPersonaIdentificacion" style="display: none">
            <label class="control-label col-lg-1"></label>
            <label class="control-label col-lg-1 text-right"><h5>Clave: <b style="color: red" title="Campo Requerido">*</b></h5></label>
            <div class="col-sm-3">
                <input type="text" class="form-control" id="buscaPersona" onkeyup="mayuscula(this);"> 
            </div>
            <label class="control-label col-lg-1 text-right"><h5>Tipo: <b style="color: red" title="Campo Requerido">*</b></h5></label>
            <div class="col-sm-3">
                <!-- consulta catalogo -->
                <SELECT id="tipoBuscaPersona" class="form-control">
                    <option value="0">SELECCIONAR...</option>
                    <option value="1">NO. CUENTA</option>
                    <option value="2">IDENTIFICACIÓN OFICIAL</option>
                    <option value="3">CURP</option>
                    <option value="4">RFC</option>
                </SELECT>
            </div>
            <input type="button" id ="btnCliente" value="Buscar" class="estilobtn gris" onclick="buscaCliente();">
            <label class="control-label col-lg-8"></label>
            <div  id="buscaOtroClienteF" style="display:none">
                <label  onclick="buscarOtroCliente();">
                    <h6 class="cambiarColor">Buscar cliente</h6></label>
                <label onclick="editarInformacion();" >
                    <h6 class="cambiarColor" id="editInfo" style="display: block">Editar Información</h6></label>
                <label  onclick="ocultarInformacion();">
                    <h6 class="cambiarColor"  id="ocultaInfo" style="display: none">Ocultar Información</h6></label>
            </div>
            <label class="control-label col-lg-9"></label>
            <div  id="editarInformacion" style="display:none">
                <!--aquí se aloja un mensaje de error en caso de que no exista un cliente con los parametro de busqueda-->
            </div>
        </div>
        <br />
    </div>
    <br />
    <div id="infoEstaticaRemitente" style="display: none">
        <div class="form-group div-azul">
            <label class="control-label col-lg-2"></label>
            <label class="control-label"> <h5>DATOS DEL REMITENTE</h5></label>
        </div>
        <label class="control-label col-lg-2"></label>
        <div  class="col-md-9">
            <div class="form-group">
                <label class="control-label col-lg-2"> <h5>Nombre: </h5></label>
                <label class="control-label col-lg-3" id="nombreFisica"></label>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-2"> <h5>CURP: </h5></label>
                <label class="control-label col-lg-3" id="curpFis"></label>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-2"> <h5>Idenficiación Oficial: </h5></label>
                <label class="control-label col-lg-4" id="identificacionNo"></label>
                <label class="control-label col-lg-2"> <h5>Tipo: </h5></label>
                <label class="control-label col-lg-2" id="tipo"></label>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-2"> <h5>Direcci&oacute;n: </h5></label>
                <label class="control-label col-lg-4" id="dirBeneficiario"></label>
                <label class="control-label col-lg-2"> <h5>Colonia: </h5></label>
                <label class="control-label col-lg-3" id="colBeneficiario"></label>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-2"> <h5>Estado: </h5></label>
                <label class="control-label col-lg-4" id="estBeneficiario"></label>
                <label class="control-label col-lg-2"> <h5>Ciudad: </h5></label>
                <label class="control-label col-lg-3" id="ciuBeneficiario"></label>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-2"> <h5>Telefono: </h5></label>
                <label class="control-label col-lg-2" id="telefonoBeneficiario"></label>
            </div>
        </div><br /><br /><!-- panel-primary-->
    </div><br /><br />
    <div class="col-md-12">
        <div id="mensajeBusqueda" style="display:none">
            <label class="control-label col-lg-3"></label>
            <label class="control-label div-rojo"> <h5 id="mensajeBusquedaError"></h5></label>
        </div> 
        <div id="infoRemitente" style="display:none">
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label div-azul"> <h5>INFORMACIÓN DEL REMITENTE</h5></label>
            </div>
            <div class="form-group">            
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>No. Cuenta:</h5></label>
                <div class="col-sm-3">
                    <label class="control-label" id="clavePersonaFisica"></label>
                </div>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Nombre:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="nombrepersonafisica" onkeyup="mayuscula(this);"> 
                </div>
                <div class="div-medida div-right">
                    <div id="huella" class="huella border-div"><p><h4>Agregar huella<b class="requerido" title="Campo Requerido">*</b> </h4></p></div>
                    <label class="control-label col-lg-5"></label>
                    <input type="button" value="Agregar" class="estilobtn gris" onclick="datosEnvio();">
                </div>
            </div>
            <br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Apellido Paterno:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="apaternofisica" onkeyup="mayuscula(this);">
                </div>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Apellido Materno:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="amaternofisica" onkeyup="mayuscula(this);">
                </div>
            </div>
            <br /><br />
            <div class="form-group">            
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>CURP:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="curpfisica" onchange="detectaCambio('curpfisica');" onkeyup="mayuscula(this);"> 
                </div>
                <label class="control-label col-lg-2"><h5>RFC:</h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="rfcfisica" onchange="detectaCambio('rfcfisica');" onkeyup="mayuscula(this);"> 
                </div>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Sexo:<b class="requerido" title="Campo Requerido">*</b></h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="sexofisica" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lSexo}" var="sexo">
                            <OPTION value="${sexo.sexoid}">${sexo.descripcion}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
                <label class="control-label col-lg-2"><h5>Fecha nacimiento:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="date" class="form-control" id="fechanacimiento" onkeyup="mayuscula(this);">
                </div>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>No. identificación:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="noidentificacion" onchange="detectaCambio('noidentificacion');" > 
                </div>
                <label class="control-label col-lg-2"><h5>Tipo de Identificación:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="tipoIdentificacion" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lTipoIdentidad}" var="tipoiden">
                            <OPTION value="${tipoiden.identificacionid}">${tipoiden.nombre}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
            </div><br /><br />
            <div class="form-group">            
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Estatus Identificación:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="statusIdentificacion" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lEstatusIdentificacion}" var="status">
                            <OPTION value="${status.estatusidentid}">${status.nombre}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
                <label class="control-label col-lg-2"><h5>Actividad:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="actividad" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lActividadEconomica}" var="actividad">
                            <OPTION value="${actividad.actividadid}">${actividad.nombre}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Email:</h5></label>
                <div class="col-sm-3">
                    <input type="email" class="form-control" id="emailfisica" onkeyup="mayuscula(this);"> 
                </div>
            </div><br /><br />
            <div class="form-group">            
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Calle y No.:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="callefisica" onkeyup="mayuscula(this);"> 
                </div>

                <label class="control-label col-lg-2"><h5>Colonia:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="coloniafisica" onkeyup="mayuscula(this);"> 
                </div>
            </div><br /><br />
            <div class="form-group">            
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>C.P.<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="codigopostalfisica" class="form-control" onchange="detectaCambioCombo('cp');">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lCodigoPostal}" var="cp">
                            <OPTION value="${cp.codigopostalid}">${cp.clave}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
                <label class="control-label col-lg-2"><h5>Ciudad:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="ciudad" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lCiudad}" var="ciudad">
                            <OPTION value="${ciudad.ciudadid}">${ciudad.nombre}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
            </div><br /><br />
            <div class="form-group">            
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Estado:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="estado" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lEstado}" var="estado">
                            <OPTION value="${estado.estadoid}">${estado.nombre}</OPTION>-->
                        </c:forEach>
                    </SELECT>
                </div>
            </div><br /><br />
            <div class="form-group">
                <label class="control-label col-lg-1"></label>
                <label class="control-label col-lg-2"><h5>Télefono:<b class="requerido" title="Campo Requerido">*</b></h5></label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="telefonofisica"> 
                </div>
                <label class="control-label col-lg-2"><h5>Tipo:<b class="requerido" title="Campo Requerido">*</b></h5></label>
                <div class="col-sm-3">
                    <!-- consulta catalogo -->
                    <SELECT id="tipoTelefonofisica" class="form-control">
                        <option value="0">SELECCIONAR...</option>
                        <c:forEach items="${lTipoTelefono}" var="tipo">
                            <OPTION value="${tipo.tipoid}">${tipo.tipotelefono}</OPTION>
                        </c:forEach>
                    </SELECT>
                </div>
            </div>
        </div> 
        <div class="form-group" id="infoValidar" style="display: none">
            <br /><br /><br /><br />
            <div class="panel panel-group">
                <br />
                <div class="form-group">
                    <label class="control-label col-lg-3 text-right"><h5>Instrumento Monetario:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                    <div class="col-sm-3">
                        <!-- consulta catalogo -->
                        <SELECT id="instrumentoMonetario" class="form-control">
                            <option value="0">SELECCIONAR...</option>
                            <c:forEach items="${lInstumento}" var="ins">
                                <OPTION value="${ins.instmonetarioid}">${ins.nombre}</OPTION>
                            </c:forEach>
                        </SELECT>
                    </div>
                    <label class="control-label col-lg-1 text-right"><h5>Servicio:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                    <div class="col-sm-3">
                        <!-- consulta catalogo -->
                        <SELECT id="tipoServicio" class="form-control">
                            <option value="0">SELECCIONAR...</option>
                            <c:forEach items="${lTipoServicio}" var="tipo">
                                <OPTION value="${tipo.tiposervicioid}">${tipo.descripcion}</OPTION>
                            </c:forEach>
                        </SELECT>
                    </div>
                </div>
                <br /><br />
                <div class="panel-group">
                    <label class="control-label col-lg-1"></label>
                    <label class="control-label col-lg-2 text-right"><h5>Uso del Dinero:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                    <div class="col-sm-3">
                        <!-- consulta catalogo -->
                        <SELECT id="usoDinero" class="form-control">
                            <option value="0">SELECCIONAR...</option>
                            <c:forEach items="${lUsoDinero}" var="uDinero">
                                <OPTION value="${uDinero.usoDineroId}">${uDinero.descripcion}</OPTION>
                            </c:forEach>
                        </SELECT>
                    </div>
                    <label class="control-label col-lg-1 text-right"><h5>Parentesco:<b class="requerido" title="Campo Requerido">*</b> </h5></label>
                    <div class="col-sm-3">
                        <!-- consulta catalogo -->
                        <SELECT id="parentesco" class="form-control">
                            <option value="0">SELECCIONAR...</option>
                            <c:forEach items="${lParentesco}" var="parentesco">
                                <OPTION value="${parentesco.parentescoid}">${parentesco.descripcion}</OPTION>
                            </c:forEach>
                        </SELECT>
                    </div>
                </div>
                <br /><br /><br />
                <div class="fondo-azul">
                    <label class="control-label col-lg-1"></label>
                    <label class="control-label"><h5>VALIDAR INFORMACI&Oacute;N:</h5></label>
                </div><br /><br /><br />
                <label class="control-label col-lg-2"></label>
                <label class="control-label col-lg-6"><input type="checkbox" id="validacion" onclick="habilitaBtnPago();">¿He validado los datos del cliente?<b class="requerido" title="Campo Requerido">*</b> </label>
                <br /><br /><br />
            </div>
        </div><br /><br />
        <div class="form-group contenedor" >
            <div class="col-md-12">
                <div class="container2">                
                    <div class="div-btn-left" id="btnAtrasPagar">
                        <input type="button" value="Atras" class="estilobtn gris" onclick="atrasPagar();">
                    </div>
                    <div class="div-btn" id="noPagarRemesa">
                        <input type="button" value="No Pagar" id="btnNoPagarRemesa" class="estilobtn gris" data-toggle="modal" data-target="#myModal">
                    </div>
                    <div class="div-btn-right" id="pagarRemesa">
                        <input type="button" value="Pagar" id="btnPagarRemesa" class="estilobtn verde" onclick="pagarRemesa();" disabled>
                    </div>
                </div>
            </div>
        </div>
    </div>               
</div>

