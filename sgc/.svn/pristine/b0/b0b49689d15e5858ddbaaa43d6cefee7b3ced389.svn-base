package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.MonedaDivisaDao;
import com.soma.transmisor.model.MonedaDivisa;
import com.soma.transmisor.service.MonedaDivisaService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Service("monedaDivisaService")
@Transactional
public class MonedaDivisaServiceImpl implements MonedaDivisaService{

    @Autowired
    MonedaDivisaDao monedaDivisaService;
    
    @Override
    public MonedaDivisa buscaMonedaDivisa(int monedaDivisaId) {
        return monedaDivisaService.buscaClave(monedaDivisaId);
    }

    @Override
    public List<MonedaDivisa> showMonedaDivisa() {
        return monedaDivisaService.showMonedaDivisa();
    }
    
}
