package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.OperacionPreocupanteDao;
import com.soma.transmisor.model.OperacionPreocupante;
import com.soma.transmisor.service.OperacionPreocupanteService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JASMIN-SOMA
 */
@Service("operacionPreocupanteService")
@Transactional
public class OperacionPreocupanteServiceImpl implements OperacionPreocupanteService{
    @Autowired
    OperacionPreocupanteDao operacionPreocupanteDao;
    private boolean estadoMetodo = false;

    @Override
    public boolean save(OperacionPreocupante operacion) {
    operacionPreocupanteDao.save(operacion);
    return estadoMetodo=true;
    }

    @Override
    public OperacionPreocupante buscaId(int operacionpid) {
    return operacionPreocupanteDao.buscaId(operacionpid);
    }

    @Override
    public List<OperacionPreocupante> showOperacionPreocupante() {
   return operacionPreocupanteDao.showOperacionPreocupante();
    }

    @Override
    public List<OperacionPreocupante> showOpePreocupanteUs(List<Integer> usuarios,List<String>descripciones) {
    return operacionPreocupanteDao.showOpePreocupanteUs(usuarios,descripciones);
    }
    
}
