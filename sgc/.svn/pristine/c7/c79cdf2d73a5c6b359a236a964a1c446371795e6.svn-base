package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.TipoTelefonoDao;
import com.soma.transmisor.model.TipoTelefono;
import com.soma.transmisor.service.TipoTelefonoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JUAN LUIS-SOMA
 */
@Service("tipoTelefonoService")
@Transactional
public class TipoTelefonoServiceImpl implements TipoTelefonoService{

    @Autowired
    TipoTelefonoDao tipoTelefonoDao;
    
    @Override
    public TipoTelefono buscaClave(int codigopostalid) {
        return  tipoTelefonoDao.buscaClave(codigopostalid);
   }

    @Override
    public List<TipoTelefono> showTipoTelefono() {
        return tipoTelefonoDao.showTipoTelefono();
    }
    
}
