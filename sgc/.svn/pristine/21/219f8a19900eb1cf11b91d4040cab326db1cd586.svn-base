                                    <div class="form-group">
                                        <label class="control-label col-lg-3"><h5>Apellido Materno:</h5></label>
                                        <div class="col-sm-3">
                                            <input type="text" class="form-control mayuscula" id="amaternobene"> 
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-3"><h5>CURP:</h5></label>
                                        <div class="col-sm-3">
                                            <input type="text" class="form-control mayuscula" id="curpbene"> 
                                        </div>
                                        <label class="control-label col-lg-2"><h5>Sexo:</h5></label>
                                        <div class="col-sm-3">
                                            <!-- consulta catalogo -->
                                            <SELECT id="sexobene" class="form-control">
                                                <option value="0">Seleccionar...</option>
                                                <c:forEach items="${lSexo}" var="sexo">
                                                    <OPTION value="${sexo.sexoid}">${sexo.descripcion}</OPTION>
                                                    </c:forEach>
                                            </SELECT>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-3"><h5>Teléfono:</h5></label>
                                        <div class="col-sm-3">
                                            <input type="number" class="form-control" id="telBeneficiario"> 
                                        </div> 
                                        <label class="control-label col-lg-2"><h5>Tipo:</h5></label>
                                        <div class="col-sm-3">
                                            <!-- consulta catalogo -->
                                            <SELECT id="tipoTelBeneficiario" class="form-control">
                                                <option value="0">Seleccionar...</option>
                                                <c:forEach items="${lTipoTelefono}" var="tipo">
                                                    <OPTION value="${tipo.tipoid}">${tipo.tipotelefono}</OPTION>
                                                    </c:forEach>
                                            </SELECT>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label col-lg-3"><h5>Calle y Número:</h5></label>
                                        <div class="col-sm-3">
                                            <input type="text" class="form-control mayuscula" id="calleBeneficiario"> 
                                        </div>

                                        <label class="control-label col-lg-2"><h5>Colonia:</h5></label>
                                        <div class="col-sm-3">
                                            <input type="text" class="form-control mayuscula" id="coloniaBeneficiario"> 
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-3"><h5>C.P.</h5></label>
                                        <div class="col-sm-3">
                                            <!-- codigoPostal -->
                                            <SELECT id="cpBeneficiario" class="form-control" onchange="detectaCambioCombo('cp');">
                                                <option value="0">SELECCIONAR...</option>
                                                <c:forEach items="${lCodigoPostal}" var="cp">
                                                    <OPTION value="${cp.codigopostalid}">${cp.clave}</OPTION>
                                                    </c:forEach>
                                            </SELECT>
                                        </div>
                                        <label class="control-label col-lg-2"><h5>Ciudad:</h5></label>
                                        <div class="col-sm-3">
                                            <!-- consulta catalogo -->
                                            <SELECT id="ciudadBeneficiario" class="form-control">
                                                <option value="0">SELECCIONAR...</option>
                                                <c:forEach items="${lCiudad}" var="ciudad">
                                                    <OPTION value="${ciudad.ciudadid}">${ciudad.nombre}</OPTION>
                                                    </c:forEach>
                                            </SELECT>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-3"><h5>Estado</h5></label>
                                        <div class="col-sm-3">
                                            <!-- consulta catalogo -->
                                            <SELECT id="estadoBeneficiario" class="form-control">
                                                <option value="0">SELECCIONAR...</option>
                                                <c:forEach items="${lEstado}" var="estado">
                                                    <OPTION value="${estado.estadoid}">${estado.nombre}</OPTION>-->
                                                    </c:forEach>
                                            </SELECT>
                                        </div>
                                    </div>




                                    <br />
                                    <div class="form-group">
                                        <label class="control-label col-lg-4" for="destino">Destino</label>
                                        <div class="col-sm-2">
                                            <!-- consulta catalogo -->
                                            <SELECT id="destino" class="form-control">
                                                <option value="0">Seleccionar...</option>
                                                <c:forEach items="${lCiudad}" var="ciudad">
                                                    <OPTION value="${ciudad.ciudadid}">${ciudad.nombre}</OPTION>
                                                    </c:forEach>
                                            </SELECT>
                                        </div>
                                        <label class="control-label col-lg-2" for="emisora">Emisora</label>
                                        <div class="col-sm-2">
                                            <input type="text" class="form-control" id="emisora" placeholder="Emisora" disabled="true"> 
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-9" ></label> 
                                        <input type="button" value="Atras" class="estilobtn gris" onclick="datosAtrasPersona();">
                                        <input type="button" value="Registrar Envió" class="estilobtn verde" onclick="registrarEnvio();">
                                        <!-- <button id="envio" class="btn btn-success" onclick="registrarEnvio();">Registrar Envió</button>-->
                                    </div>
                                </div>


                            </form> 
                            <table id='result'>
                            </table>
                        </div><!--modal body-->
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>