<div class="container">
    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog" data-backdrop="static" data-keyboard="false">

        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <div class="form-group div-azul">
                        <h3>SELECCIONE LA OPCI&Oacute;N DE LA CAUSA/RECHAZO DEL PAGO</h3>
                    </div>
                </div>
                <div class="modal-body">
                    <!--Formilario para agregar usuario -->
                    <form class="form-horizontal"  modelAttribute="user" id="frmCausaRechazo">

                        <label class="control-label col-lg-3"><h5>Causa/Rechazo:</h5></label>
                        <div class="col-sm-5">
                            <!-- consulta catalogo -->
                            <SELECT id="causaRechazoId" class="form-control">
                                <option value="0">SELECCIONAR...</option>
                                <c:forEach items="${lCausaRechazo}" var="cr">
                                    <OPTION value="${cr.causaRechazoId}">${cr.descripcion}</OPTION>
                                    </c:forEach>
                            </SELECT>
                        </div>
                        <br /><br />
                    </form>
                </div><!--modal body-->
                <div class="modal-footer">
                    <div class="form-group">
                        <button id="noPagarRemesaBtn" onclick="pagarRemesa();" class="btn btn-success" >Aceptar</button>
                        <button type="button" onclick="cerrarModal();">Cerrar</button>
                    </div>
                </div><!--modal footer-->
