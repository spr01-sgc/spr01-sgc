            || rfc == '' || calle == '' || colonia == '') {
        alert("No dede de quedar un campo vacío");
        return;
    }

    if (codigopostal == 0) {
        alert("Debes de seleccionar un codigo postal")
    }
    var datos = [razonsocial,
        nombrecorto,
        correoelectronico,
