    var apaterno = $("#apaterno").val();
    var amaterno = $("#amaterno").val();
    var curp = $("#curp").val();
    var sexo = $("#sexo").val();
    var telefono = $("#numtelefono").val();
    var tipotelefono = $("#telefono").val();


    //falta extraer los datos de los campos de direccion y persona


