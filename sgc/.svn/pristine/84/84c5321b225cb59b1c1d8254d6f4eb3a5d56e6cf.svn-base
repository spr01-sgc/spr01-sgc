package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.EstadoDao;
import com.soma.transmisor.model.Estado;
import com.soma.transmisor.service.EstadoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Service("estadoService")
@Transactional
public class EstadoServiceImpl implements EstadoService{

    @Autowired
    EstadoDao estadoDao;
    
    @Override
    public Estado buscaClave(int estadoId) {
        return estadoDao.buscaClave(estadoId);
    }

    @Override
    public List<Estado> showEstado() {
        return estadoDao.showEstado();
    }
    
}
