<div class="container">
    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="border-color:#0000FF">
                <div class="modal-header" style="border-color:#0000FF; background: linear-gradient(to bottom, rgba(252,234,187,1) 0%, rgba(248,181,0,1) 51%, rgba(252,205,77,1) 64%, rgba(251,223,147,1) 100%);" >
                    <h3> Personas Politicamente expuestas</h3>
                </div>
                <div class="modal-body">
                    <!--Formilario para agregar usuario -->
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label  class="control-label col-sm-2">Fecha Vigente: </label>
                            <div class="col-sm-3">
                                <input type="date" class="form-control" id="fechPep" placeholder="Ingrese la fecha correspondiente">
                            </div>                            
                            <label  class="control-label col-sm-1">Cargo:</label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="cargoPep" class="form-control">
                                    <option value="0">Seleccione</option>
                                    <c:forEach items="${lCargoPolitico}" var="cargo">
                                        <option value="${cargo.cargoid}">${cargo.nombre} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>  
                             <label  class="control-label col-sm-1">Ciudad:</label>
                            <div class="col-sm-3">
                                <!-- consulta catalogo -->
                                <select id="ciudadPep" class="form-control">
                                    <option value="0">Seleccione</option>
                                    <c:forEach items="${lCiudad}" var="ciudad">
                                        <option value="${ciudad.ciudadid}">${ciudad.nombre} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>  
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" >Nombre</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="nomPep" placeholder="Enter number expedient">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2">Apellido Paterno</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apPatPep" placeholder="Enter ">
                            </div>
                            <label class="control-label col-sm-2">Apellido Materno</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apMatPep" placeholder="Enter ">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" >CURP</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="curpPep" placeholder="Enter curp">
                            </div>
                            <label  class="control-label col-sm-1">Sexo:</label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="sexoPep" class="form-control">
                                    <option value="0">Seleccione sexo </option>
                                    <c:forEach items="${lSexo}" var="sexo">
                                        <option value="${sexo.sexoid}">${sexo.descripcion} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>   
                                                          
                            <label  class="control-label col-sm-1">Estatus:</label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="delitoPba" class="form-control">
                                    <option value="0">Seleccione el Estatus</option>
                                    <c:forEach items="${lEstPba}" var="estatus">
                                        <option value="${estatus.estatuspbapepid}">${estatus.descripcion} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>                           
                        </div> 
                        
                        </div>
                    </form> 
                </div><!--modal body-->
                <div class="modal-footer">
                    <div class="form-group">
                        <button id="guardar" type="button" class="btn btn-success" onclick="agregarPba();" data-dismiss="modal" >Guardar</button>
                        <button id="actualizar" onclick="actualizarPba();" class="btn btn-primary" disabled="true" >Actualizar</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- ventana modal-->                               
</div> <!-- panel-container-->


