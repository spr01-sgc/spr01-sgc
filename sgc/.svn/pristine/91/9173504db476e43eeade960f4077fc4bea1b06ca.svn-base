<div class="container">   
    <div class="panel-group">
        <div class="panel panel-primary">
            <div class="panel-heading">PEP</div>
            <div id="contenidoPantalla" class="panel-body">        
                <div class="row">
                    <!-- Listado de pba -->
                    <div  class="col-md-12">
                        <div class="table-responsive">
                            <table id="tableCatalogo" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>Fecha Vigente</th>
                                        <th style="display:none">CargoId</th>                                
                                        <th>Cargo</th>
                                        <th style="display:none">CiudadId</th>
                                        <th>Ciudad</th>  
                                        <th>Nombres</th>
                                        <th>Apellido Paterno</th>
                                        <th>Apellido Materno</th>  
                                        <th>CURP</th>
                                        <th style="display:none">Sexo ID</th>
                                        <th>Sexo</th>
                                        <th style="display:none">EstatusId</th>
                                        <th>Estatus</th>                                        
                                        <th>Actualizar</th>

                                    </tr>
                                <thead>
                                <tbody id="bodytabla">
                                    <c:forEach items="${lPep}" var="pep">
                                        <tr>
                                            <td id="idpep">${pep.pepid}</td>
                                            <td>${pep.fechavigente}</td>
                                            <td style="display:none">${pep.cargoPolitico.cargoid}</td>
                                            <td>${pep.cargoPolitico.nombre}</td>
                                            <td style="display:none">${pep.ciudad.ciudadid}</td>
                                            <td>${pep.ciudad.nombre}</td> 
                                            <td>${pep.personaid.nombre}</td>                                        
                                            <td>${pep.personaid.apaterno}</td> 
                                            <td>${pep.personaid.amaterno}</td> 
                                            <td>${pep.personaid.curp}</td> 
                                            <td style="display:none"> ${pep.personaid.sexoid.sexoid}</td> 
                                            <td> ${pep.personaid.sexoid.descripcion}</td>                                          
                                            <td style="display:none"> ${pep.estatuspbapepid.estatuspbapepid}</td> 
                                            <td> ${pba.estatuspbapepid.descripcion}</td>                                                                                   
                                            
                                            <td><button id="mostrar" onclick="mostrarNombre();" data-toggle="modal" data-target="#myModal" class="btn btn-warning">
                                                    <span class="glyphicon glyphicon-pencil"></span> </button></td>

                                        </tr>

                                    </c:forEach>

                                </tbody>

                            </table>

                        </div>     <!-- Table-->      
                        <button id="agregar" class="btn btn-info" data-toggle="modal" data-target="#myModal"> 
                            <span class="glyphicon glyphicon-file"></span> agregar</button>
                        <input type="hidden" name="${_csrf.parameterName}"
                               value="${_csrf.token}" />
                    </div><!--col-m-12 -->
                    <!--Familiar -->
