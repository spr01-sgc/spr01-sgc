package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.UsoDineroDao;
import com.soma.transmisor.model.UsoDinero;
import com.soma.transmisor.service.UsoDineroService;
import java.util.List;
import javax.transaction.Transactional;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

/**
 *
 * @author LUIS-SOMA
 */
@Service("usoDineroService")
@Transactional
public class UsoDineroServiceImpl implements UsoDineroService{
    
    @Autowired
    UsoDineroDao usoDineroDao;

    @Override
    public UsoDinero buscaId(int usoDineroId) {
        return usoDineroDao.buscaId(usoDineroId);
    }

    @Override
    public List<UsoDinero> showUsoDinero() {
        return usoDineroDao.showUsoDinero();
    }
    
}
