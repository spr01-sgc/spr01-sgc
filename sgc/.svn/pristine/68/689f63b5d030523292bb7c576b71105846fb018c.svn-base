<%@include file="index.jsp" %>
<br />
<br />
<br />
<div class="container">   
    <div class="panel-group">
        <div class="panel panel-primary">
            <div class="panel-heading">Operaciones Relevantes</div>
            <div id="contenidoPantalla" class="panel-body">        
                <div class="row">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <!--    <label class="control-label col-sm-1" for="sucursal">Sucursal: </label>
                              <div class="col-sm-2">
                                 consulta catalogo T. Operacion 
                                  <SELECT class="form-control">
                                      <option value="0">Seleccionar </option>
                            <c:forEach items="${lSucursal}" var="sucursal">
                                <option value="${sucursal.sucursalid}">${sucursal.nombresucursal}</option>
                            </c:forEach>
                        </SELECT>
                    </div>-->

                            <label  class="control-label col-sm-1">Fecha inicio:</label>
                            <div class="col-sm-2">
                                <input type="date" class="form-control" id="fechIni">
                            </div>                                 

                            <label  class="control-label col-sm-2">Fecha Termino:</label>
                            <div class="col-sm-2">
                                <input type="date" class="form-control" id="fechTer" onchange="movimientosRelevantes();">
                            </div>
                        </div>
                        <div class="form-group">
                            <label  class="control-label col-sm-1">Valor del dolar: </label>
                            <div class="col-sm-2">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="glyphicon glyphicon-usd"></i></span>
                                    <input type="number" class="form-control" id="valorDolarPld" onkeypress="conversion();"value="18.73"/>
                                </div>
                            </div> 
                            <label  class="control-label col-sm-1" id="dolares">Dolares:  </label>
                            <div class="col-sm-2">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="glyphicon glyphicon-usd"></i></span>
                                    <input type="number"  class="form-control" id="dolaresPld" disabled="true" value="5000.00"/> 
                                </div>
                            </div>
                            <label  class="control-label col-sm-2" id="pesos">Pesos MXN: </label> 
                            <label  class="control-label col-sm-1" id="pesosPld"> $00.00 </label> 

                        </div>
                        <input type="hidden" name="${_csrf.parameterName}"
                               value="${_csrf.token}" />
                    </form>
                </div><!-- ROW-->
            </div>
        </div>
    </div>
</div><!--Container -->
<div id="tblRelevantes" style="display: none">
    <div class="container">
        <div  class="col-md-12">
            <div class="table-responsive">
                <h2>Movimientos Relevantes</h2>
                <p>Clientes y sus movimientos relevantes:</p>            
                <table id="tblConRel" class="table table-hover">
                    <thead>
                        <tr>
                            <th>No.</th>
                            <th>Clave de Cliente</th>
                            <th>Remitente</th>
                            <th>Deposito</th>
                            <th>Retiro</th>
                            <th>Beneficiario</th>
                        </tr>
                    </thead>
                    <tbody>


                    </tbody>
                </table>
            </div><!-- table responsive-->
        </div><!-- col-12-->
    </div> <!--contaniner-->
    <div class="container">
        <button id="download" onclick="reporteRelevantes('csv')" class="btn btn-success">
            CSV</button>
        <button id="download" onclick="reporteRelevantes('excel');" class="btn btn-success">
            Excel</button>
    </div>
</div>
<!--Tabla Notificacion de operación -->
<div id="tblNotifR" style="display:inline">
<div class="container" >
    <div  class="col-md-12">
        <div class="table-responsive">
            <h2>Detección Operación Relevantes</h2>           
            <table id="tblNotRel" class="table table-hover">
                <thead>
                    <tr>
                        <th>No.</th>
                        <th>Clave de Cliente</th>
                        <th>Deposito</th>
                        <th>Retiro</th>
                        <th>Beneficiario</th>
                    </tr>
                </thead>
                <tbody>
                    <c:forEach items="${lOpRel}" var="relevante" varStatus="contador">
                        <tr>
                            <td>${contador.count}</td>
                            <td>${relevante[0]}</td>
                            <td>${relevante[2]}</td>
                            <td>${relevante[3]}</td>                                                
                            <td>${relevante[1]}</td>
                        </tr>
                    </c:forEach>
                </tbody>
            </table>
        </div><!-- table responsive-->
    </div><!-- col-12-->
</div> <!--contaniner-->
</div>
<!-- Tabla para el reporte -->

<div id="tblORelevantes" style="display: none">
    <div class="container">
        <div  class="col-md-12">
            <div class="table-responsive">
                <h2>Movimientos Relevantes</h2>
                <p>Clientes y sus movimientos relevantes:</p>            
                <table id="tblOpRel" class="table table-hover">
                    <thead>
                        <tr>
                            <th>Tipo de Reporte </th>
                            <th>Periodo del reporte</th>
                            <th>Folio</th>
                            <th>Organo supervisor</th>
                            <th>Clave del sujeto obligado</th>
                            <th>Localidad</th>
                            <th>Sucrsal</th>
                            <th>Tipo de Operacion</th>
                            <th>Instumento Monetario</th>
                            <th>Numero de cuenta contrato u operacion</th>
                            <th>Monto</th>
                            <th>Moneda</th>
                            <th>Fecha de la operacion</th>
                            <th>Fecha detección de la operación</th>
                            <th>Nacionalidad</th>
                            <th>Tipo de Persona</th>
                            <th>Razon social o denominacion</th>
                            <th>Nombre</th>
                            <th>Apellido Paterno</th>
                            <th>Apellido Materno</th>
                            <th>RFC</th>
                            <th>CURP</th>
                            <th>Fecha de nacimiento o constitución</th>
                            <th>Domicilio</th>
                            <th>Colonia</th>
                            <th>Ciudad</th>
                            <th>Telefono</th>
                            <th>Actividad Economica</th>                            
                            <th>Agente o Apoderado Nombre</th>
                            <th>Apellido Paterno</th>
                            <th>Apellido Materno</th>
                            <th>RFC</th>
                            <th>CURP</th>
                            <th>Consecutivo de cuentas</th>                            
                            <th>Número de cuenta</th>
                            <th>Clave del sujeto obligado</th>
                            <th>Nombre del titular de la cuenta</th>
                            <th>Apellido Paterno</th>
                            <th>Apellido Materno</th>
                            <th>Descripción de la operación</th>
                            <th>Razones por las que se consideraa Inu o Pre</th>
                        </tr>
                    </thead>
                    <tbody id="tbRel">
                    </tbody>
                </table>
            </div><!-- table responsive-->
        </div>
    </div>
</div>
</div>
</body>
</html>