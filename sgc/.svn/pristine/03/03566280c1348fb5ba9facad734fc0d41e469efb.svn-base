package com.soma.transmisor.model;

import java.io.Serializable;
import java.util.Date;
import java.util.List;
import javax.persistence.CascadeType;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.OneToMany;
import javax.persistence.OneToOne;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;
import org.hibernate.annotations.ForeignKey;

/**
 *
 * @author JASMIN-SOMA
 */
@Entity
@Table(name = "pba")
public class Pba implements Serializable {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    @Column(name = "pbaid")
    private Integer pbaid;
    @Column(name = "noexpediente")
    private String noexpediente;
    @Column(name = "fechabusqueda")
    @Temporal(TemporalType.DATE)
    private Date fechabusqueda;

    //Establece relacion con Persona
    @OneToOne(cascade = CascadeType.ALL)
    @ForeignKey(name = "personaid_fk")
    private Persona personaid;

    //delito
    @ManyToOne
    @JoinColumn(name = "delitoid")
    private Delito delitoid;

    //relacion con organizacion     
    @ManyToOne
    @JoinColumn(name = "organizacionid")
    private Organizacion organizacionid;
    //relacion con pais
    @OneToOne
    @ForeignKey(name = "paisid_fk")
    private Pais paisid;        
      //Establece relacion con Pba
    @OneToOne(cascade = CascadeType.ALL)
    @ForeignKey(name = "informacionadicionalid_fk")
    private InformacionAdicional informacionadicionalid;
       //Establece relacion con ct_estatupepba
    @ManyToOne
    @ForeignKey(name = "estatuspbapepid_fk")
    private EstatusPba estatuspbapepid;

    public Integer getPbaid() {
        return pbaid;
    }

    public void setPbaid(Integer pbaid) {
        this.pbaid = pbaid;
    }

    public String getNoexpediente() {
        return noexpediente;
    }

    public void setNoexpediente(String noexpediente) {
        this.noexpediente = noexpediente;
    }

   
    public Date getFechabusqueda() {
        return fechabusqueda;
    }

    public void setFechabusqueda(Date fechabusqueda) {
        this.fechabusqueda = fechabusqueda;
    }

    public Persona getPersonaid() {
        return personaid;
    }

    public void setPersonaid(Persona personaid) {
        this.personaid = personaid;
    }

    public Delito getDelitoid() {
        return delitoid;
    }

    public void setDelitoid(Delito delitoid) {
        this.delitoid = delitoid;
    }

    public Organizacion getOrganizacionid() {
        return organizacionid;
    }

    public void setOrganizacionid(Organizacion organizacionid) {
        this.organizacionid = organizacionid;
    }

    public Pais getPaisid() {
        return paisid;
    }

    public void setPaisid(Pais paisid) {
        this.paisid = paisid;
    }

    public EstatusPba getEstatuspbapepid() {
        return estatuspbapepid;
    }

    public void setEstatuspbapepid(EstatusPba estatuspbapepid) {
        this.estatuspbapepid = estatuspbapepid;
    }

    public InformacionAdicional getInformacionadicionalid() {
        return informacionadicionalid;
    }

    public void setInformacionadicionalid(InformacionAdicional informacionadicionalid) {
        this.informacionadicionalid = informacionadicionalid;
    }

    @Override
    public String toString() {
        return "Pba{" + "pbaid=" + pbaid + ", noexpediente=" + noexpediente + ", fechabusqueda=" + fechabusqueda + ", personaid=" + personaid + ", delitoid=" + delitoid + ", organizacionid=" + organizacionid + ", paisid=" + paisid + ", informacionadicionalid=" + informacionadicionalid + ", estatuspbapepid=" + estatuspbapepid + '}';
    }


    
}
