package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.CodigoPostalDao;
import com.soma.transmisor.model.CodigoPostal;
import com.soma.transmisor.service.CodigoPostalService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JUAN LUIS-SOMA
 */
@Service("codigoPostalService")
@Transactional
public class CodigoPostalServiceImpl implements CodigoPostalService{

    @Autowired
    CodigoPostalDao codigoPostalDao;
    
    @Override
    public CodigoPostal buscaClave(int codigopostalid) {
        return  codigoPostalDao.buscaClave(codigopostalid);
   }

    @Override
    public List<CodigoPostal> showCodigoPostal() {
       return codigoPostalDao.showCodigoPostal();
    }
    
}
