package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.CtCausaRechazoDao;
import com.soma.transmisor.model.ctCausaRechazo;
import com.soma.transmisor.service.CtCausaRechazoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Service("ctCausaRechazoService")
@Transactional
public class CtCausaRechazoServiceImpl implements CtCausaRechazoService{
    
    @Autowired
    CtCausaRechazoDao ctCausaRechazoDao;

    @Override
    public ctCausaRechazo buscaCausaRechazo(int causaRechazoId) {
        return ctCausaRechazoDao.buscaCausaRechazo(causaRechazoId);
    }

    @Override
    public List<ctCausaRechazo> showCausaRechazo() {
        return ctCausaRechazoDao.showCausaRechazo();
    }
    
}
