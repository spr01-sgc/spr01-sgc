package com.soma.transmisor.dao.impl;

import com.soma.transmisor.dao.ComisonDao;
import com.soma.transmisor.dao.HibernateDao;
import com.soma.transmisor.model.Comision;
import org.springframework.stereotype.Repository;

/**
 *
 * @author LUIS-SOMA
 */
@Repository("comisonDao")
public class ComisonDaoImpl extends HibernateDao<Integer, Comision> implements ComisonDao{

