package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.OrganizacionDao;
import com.soma.transmisor.model.Organizacion;
import com.soma.transmisor.service.OrganizacionService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JASMIN-SOMA
 */
@Service("organizacionService")
@Transactional
public class OrganizacionServiceImpl implements OrganizacionService{
@Autowired
OrganizacionDao organizacionDao;
    @Override
    public Organizacion buscaId(int organizacionid) {
    return organizacionDao.buscaId(organizacionid);
    }

    @Override
    public List<Organizacion> showOrganizacion() {
    return organizacionDao.showOrganizacion();
    }
    
}
