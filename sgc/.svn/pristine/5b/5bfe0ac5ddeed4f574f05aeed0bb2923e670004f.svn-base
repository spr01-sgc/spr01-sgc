package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.BitacoraPagoDao;
import com.soma.transmisor.model.BitacoraPago;
import com.soma.transmisor.service.BitacoraPagoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JASMIN-SOMA
 */
@Service("bitacoraPagoService")
@Transactional
public class BitacoraPagoServiceImpl implements BitacoraPagoService{
@Autowired
BitacoraPagoDao bitacoraPagoDao;
  private boolean estadoMetodo = false;
    @Override
    public boolean save(BitacoraPago bitacoraP) {
  bitacoraPagoDao.save(bitacoraP);
        return estadoMetodo = true;
    }

    @Override
    public BitacoraPago buscaId(int bitapagoid) {
    return bitacoraPagoDao.buscaId(bitapagoid);
    }

    @Override
    public List<BitacoraPago> showBitacoraP() {
   return bitacoraPagoDao.showBitacoraP();
    }

    @Override
    public BitacoraPago buscarPorRemesa(int pagoId) {
    return bitacoraPagoDao.buscarPorRemesa(pagoId);
    }
    
}
