package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.ClienteDao;
import com.soma.transmisor.model.Cliente;
import com.soma.transmisor.service.ClienteService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author LUIS-SOMA
 */
@Repository("clienteSeervice")
@Transactional
public class ClienteServiceImpl implements ClienteService{
    @Autowired
    ClienteDao clienteDao;
    @Override
    public Cliente buscaClave(String clave) {
        return clienteDao.buscaCLave(clave);
    }

    @Override
    public Cliente clave() {
        return clienteDao.clave();
    }

    @Override
    public Cliente buscaRFC(String rfc) {
        return clienteDao.buscaRFC(rfc);
    }
    
}
