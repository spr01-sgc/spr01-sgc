<%@include file="index.jsp" %>
<br />
<br />
<br />
<div class="container">   
    <div class="panel-group">
        <div class="panel panel-primary">
            <div class="panel-heading">Listado de Entidad</div>
            <div id="contenidoPantalla" class="panel-body">
                <div class="row">
                    <div class="form-group">
                        <div class="btn btn-group">
                            <button id="agregar" class="btn btn-info" data-toggle="modal" data-target="#myModal"> 
                                <span class="glyphicon glyphicon-plus"></span>Agregar Entidad</button></div>
                        <input type="text" class="form-control pull-right" style="width:20%"
                               id="search" onkeyup="busquedaTbl('tableEntidad');" placeholder="Busqueda">
                    </div>
                    <!-- Listado de Entidades -->
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table id="tableEntidad" class="table table-striped">
                                <thead>
                                    <tr>
                                        <td style="display:none">ID</td>
                                        <td>No.</td>
                                        <td>Organo supervisor</td>
                                        <td>Clave Entidad</td>
                                        <td>Raz&oacute;n Social</td>
                                        <td>Nombre corto</td>
                                        <td>Correo electronico</td>
                                        <td>RFC</td>
                                        <td>Calle</td>
                                        <td>Colonia</td>
                                        <td style="display:none">ID CP</td>
                                        <td>C&oacute;digo Postal</td>
                                        <td>Encargado</td>
                                        <td>Apellido Paterno</td>
                                        <td>Apelido Materno</td>
                                        <td>CURP</td>
                                        <td style="display:none">ID sexo</td>
                                        <td>Sexo</td>
                                        <td style="display: none">ID Tipo Tel&eacute;fono</td>
                                        <td>Tipo de Tel&eacute;fono</td>
                                        <td style="display: none">Id tel&eacute;fono</td>
                                        <td>No.Tel&eacute;fono</td>
                                        <td style="display: none">ID Tipo Tel&eacute;fono</td>
                                        <td>Tipo de Tel&eacute;fono</td>
                                        <td style="display: none">Id tel&eacute;fono</td>
                                        <td>No.Tel&eacute;fono</td>
                                        <td></td>
                                        <td></td>

                                    </tr>
                                <thead>
                                <tbody>
                                    <c:forEach items="${lEntidad}" var="entidad" varStatus="cont">
                                        <tr>
                                            <td style="display:none">${entidad.entidadid}</td>
                                            <td>${cont.count}</td>
                                            <td>${entidad.claveOrganoSupervisor}</td>
                                            <td>${entidad.claveEntidad}</td>
                                            <td>${entidad.razonsocial}</td>
                                            <td>${entidad.nombrecorto}</td>
                                            <td>${entidad.correoelectronico}</td>
                                            <td>${entidad.rfc}</td>
                                            <td>${entidad.direccionid.calle}</td>
                                            <td>${entidad.direccionid.colonia}</td>
                                            <td style="display:none">${entidad.direccionid.codigopostalid.codigopostalid}</td>
                                            <td>${entidad.direccionid.codigopostalid.clave}</td>
                                            <td>${entidad.personaid.nombre}</td>
                                            <td>${entidad.personaid.apaterno}</td>
                                            <td>${entidad.personaid.amaterno}</td>
                                            <td>${entidad.personaid.curp}</td>
                                            <td style="display:none">${entidad.personaid.sexoid.sexoid}</td> 
                                            <td>${entidad.personaid.sexoid.descripcion}</td>
                                            <c:set var = "tel" scope = "session" value = ""/>
                                            <c:set var = "tipo" scope = "session" value = ""/>
                                            <c:set var = "tel2" scope = "session" value = ""/>
                                            <c:set var = "tipo2" scope = "session" value = ""/>
                                            <c:set var = "telID" scope = "session" value = ""/>
                                            <c:set var = "tipoID" scope = "session" value = ""/>
                                            <c:set var = "telID2" scope = "session" value = ""/>
                                            <c:set var = "tipoID2" scope = "session" value = ""/>
                                            <c:forEach items="${entidad.telefonoid}" var="telefono" varStatus="contador">  
                                                <c:choose> 
                                                    <c:when test="${entidad.telefonoid==''}"> 
                                                <p style="display: none"> ${tipo = ''}</p>
                                                <p style="display: none"> ${tel = ''}</p>
                                                <p style="display: none"> ${tipo2 = ''}</p>
                                                <p style="display: none"> ${tel2 = ''}</p>
                                                <p style="display: none"> ${tipoID = ''}</p>
                                                <p style="display: none"> ${telID = ''}</p>
                                                <p style="display: none"> ${tipoID2 =''}</p>
                                                <p style="display: none"> ${telID2 = ''}</p>
                                            </c:when> 
                                            <c:otherwise>
                                                <c:if test="${entidad.telefonoid!=''}">
                                                    <c:if test="${contador.count==1}">
                                                        <p style="display: none"> ${tipoID =telefono.tipoid.tipoid}</p>
                                                        <p style="display: none"> ${telID = telefono.telefonoid}</p>
                                                        <p style="display: none"> ${tipo =telefono.tipoid.tipotelefono}</p>
                                                        <p style="display: none"> ${tel = telefono.numTelefono}</p>
                                                    </c:if>
                                                    <c:if test="${contador.count==2}">
                                                        <p style="display: none"> ${tipoID2 =telefono.tipoid.tipoid}</p>
                                                        <p style="display: none"> ${telID2 = telefono.telefonoid}</p>
                                                        <p style="display: none"> ${tipo2 =telefono.tipoid.tipotelefono}</p>
                                                        <p style="display: none"> ${tel2 = telefono.numTelefono}</p>
                                                    </c:if>
                                                </c:if>
                                            </c:otherwise>
                                        </c:choose>  </c:forEach> 
                                    <td style="display: none"><c:out value = "${tipoID}"/></td>                                                        
                                    <td><c:out value = "${tipo}"/></td>
                                    <td style="display: none"><c:out value = "${telID}"/></td>
                                    <td><c:out value = "${tel}"/></td>
                                    <td style="display: none"><c:out value = "${tipoID2}"/></td>                                                        
                                    <td><c:out value = "${tipo2}"/></td>
                                    <td style="display: none"><c:out value = "${telID2}"/></td>
                                    <td><c:out value = "${tel2}"/></td>


                                    <td><button id="mostrar" onclick="completaInputsEntidad();" data-toggle="modal" data-target="#myModal" class="btn btn-warning">
                                            <span class="glyphicon glyphicon-pencil"></span></button></td>
                                    <td><button id="eliminar" class="btn btn-danger" onclick="eliminarEntidad(${entidad.entidadid});">
                                            <span class="glyphicon glyphicon-remove"></span></button></td>
                                    </tr>
                                </c:forEach>
                                </tbody>
                            </table>
                        </div>
                        <input type="hidden" name="${_csrf.parameterName}"
                               value="${_csrf.token}" />
                    </div>
                </div>
            </div><!--class body -->
        </div><!-- panel-primary-->
    </div>  <!-- panel-group-->  
</div><!--class container -->
<br />
<div class="container">

    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog"data-backdrop="static" 
         data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h3>Entidades Financieras</h3>
                </div>
                <div class="modal-body">
                    <!--Formilario para agregar usuario -->
                    <form class="form-horizontal" id="frmEntidad">
                        <div class="form-group">
                            <label class="control-label col-sm-3"><h4>Datos de la empresa</h4></label>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" for="organoS" >Clave del &oacute;rgano supervisor:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-md-4">
                                <input id="organoS" class="form-control"placeholder="CASFIM">
                            </div>
                            <label class="control-label col-sm-1" for="claveEntidad">Clave entidad:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="claveEnt" placeholder="Clave asignada por el SAT">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" for="razonsocial">Raz&oacute;n Social:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-md-6" style="display:none">
                                <input id="razonsocialid" class="form-control">
                            </div>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="razonsocial" placeholder="">
                            </div>

                            <label class="control-label col-sm-1" for="nombrecorto">Nombre Corto:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="nombrecorto" placeholder="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-2" for="correoelectronico">Email:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="email" class="form-control" id="correoelectronico" placeholder="">
                            </div>
                            <label class="control-label col-sm-1" for="ref">RFC:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="rfc" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" for="numtelefono">Tel&eacute;fono:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div>
                                <input type="number" class="form-control"  style="display:none" id="idtel" placeholder="IDTele">
                            </div>
                            <div class="col-sm-4">
                                <input type="number" class="form-control" id="numtelefono" placeholder="">
                            </div>

                            <label  class="control-label col-sm-1">Tipo Tel&eacute;fono:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <!-- consulta catalogo -->
                                <SELECT id="telefono" class="form-control">
                                    <option value="0">Seleccionar...</option>
                                    <c:forEach items="${lTipoTelefono}" var="tipo">
                                        <OPTION value="${tipo.tipoid}">${tipo.tipotelefono}</OPTION>
                                        </c:forEach>
                                </SELECT>
                            </div>
                        </div><!--segundo numero -->
                        <div class="form-group">
                            <label class="control-label col-sm-2" for="numtelefono">Tel&eacute;fono: </label>
                            <div>
                                <input type="number" class="form-control"  style="display:none" id="idtel2" placeholder="IDTele">
                            </div>
                            <div class="col-sm-4">
                                <input type="number" class="form-control" id="numtelefono2" placeholder="">
                            </div>

                            <label  class="control-label col-sm-1">Tipo Tel&eacute;fono:</label>
                            <div class="col-sm-4">
                                <!-- consulta catalogo -->
                                <SELECT id="telefono2" class="form-control">
                                    <option value="0">Seleccionar...</option>
                                    <c:forEach items="${lTipoTelefono}" var="tipo">
                                        <OPTION value="${tipo.tipoid}">${tipo.tipotelefono}</OPTION>
                                        </c:forEach>
                                </SELECT>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3"> <h4>Direcci&oacute;n Sucursal</h4></label>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" for="calle">Calle y n&uacute;mero:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="calle" placeholder="">
                            </div>

                            <label class="control-label col-sm-1" for="calle">Colonia: <b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="colonia" placeholder="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label  class="control-label col-sm-2">C&oacute;digo Postal:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-9">
                                <!-- consulta catalogo -->
                                <SELECT id="codigopostal" class="form-control">
                                    <option value="0">Seleccionar...</option>
                                    <c:forEach items="${lCodigoPostal}" var="cp">
                                        <OPTION value="${cp.codigopostalid}">${cp.clave}</OPTION>
                                        </c:forEach>
                                </SELECT>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3"> <h4>Persona encargada</h4></label>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" for="nombre">Nombre:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="nombre" placeholder="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-2" for="apaterno">Apellido Paterno:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apaterno" placeholder="">
                            </div>

                            <label class="control-label col-sm-1" for="amaterno">Apellido Materno:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="amaterno" placeholder="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-2" for="curp">CURP:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="curp" placeholder="">
                            </div>

                            <label  class="control-label col-sm-1">Sexo:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <!-- consulta catalogo -->
                                <SELECT id="sexo" class="form-control">
                                    <option value="0">Seleccionar...</option>
                                    <c:forEach items="${lSexo}" var="sexo">
                                        <OPTION value="${sexo.sexoid}">${sexo.descripcion}</OPTION>
                                        </c:forEach>
                                </SELECT>
                            </div>
                        </div>                               
                    </form> 
                </div><!--modal body-->
                <div class="modal-footer">
                    <div class="form-group">
                        <button id="guardar" class="btn btn-success" onclick="agregarEntidad();">Guardar</button>
                        <button id="actualizar" onclick="actualizarEntidad();" class="btn btn-primary" disabled="true">Actualizar</button>
                        <a id="entidadC" href="<c:url value='/entidad'/>"><button type="button" class="btn btn-default" onclick="limpiarForm('frmEntidad');">Cerrar</button></a>
                    </div>
                </div><!--modal footer-->
            </div><!--modal6-container-->
        </div><!--modal dialog-->
    </div><!--modalfade-->
</div><!--container-->

</body>

</html>