<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>

<jsp:include page="index.jsp" flush="true"/>
<br />
<div id="pep1" style="display: block">
    <div class="container" >   
        <div class="panel-group">
            <div class="panel panel-primary">
                <div class="panel-heading">PEP</div>
                <div id="contenidoPantalla" class="panel-body">        
                    <div class="row">
                        <!-- Listado de pba -->
                        <div  class="col-md-12">
                             <div class="form-group">
                             <div class="btn btn-group">
                            <button id="agregarPep" class="btn btn-info" data-toggle="modal" data-target="#myModal"> 
                                <span class="glyphicon glyphicon-plus"></span>Agregar PEP</button>
                            <!--    <input type="button" value="Ver Familiares"  class="estilobtn gris" onclick="verTablaF()" id="mostrarFamiliar"  style="display: block; float:right"> -->
                            <button type="text" class="btn btn-info" onclick="verTablaF()" id="mostrarFamiliar"><span class="glyphicon glyphicon-eye-open"></span> Ver Familiares</button>
                             </div> <input type="text" class="form-control pull-right" style="width:20%"
                                   id="search" onkeyup="busquedaTbl('tablePEP');" placeholder="Busqueda">
                        </div>
                            <div class="table-responsive">
                                <table id="tablePEP" class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th style="display: none">ID</th>
                                            <th>No</th>
                                            <th>Fecha Vigente</th>
                                            <th style="display:none">CargoId</th>                                
                                            <th>Cargo</th>
                                            <th style="display:none">CiudadId</th>
                                            <th>Ciudad</th>  
                                            <th>Nombres</th>
                                            <th>Apellido Paterno</th>
                                            <th>Apellido Materno</th>  
                                            <th>CURP</th>
                                            <th style="display:none">Sexo ID</th>
                                            <th>Sexo</th>
                                            <th style="display:none">EstatusId</th>
                                            <th>Estatus</th>                                        
                                            <th>Actualizar</th>

                                        </tr>
                                    </thead>
                                    <tbody id="bodytabla">
                                        <c:forEach items="${lPep}" var="pep" varStatus="cont">
                                            <tr>
                                                <td id="idpep" style="display: none">${pep.pepid}</td>
                                                <td>${cont.count}</td>
                                                <td>${pep.fechavigente}</td>
                                                <td style="display:none">${pep.cargoPolitico.cargoid}</td>
                                                <td>${pep.cargoPolitico.nombre}</td>
                                                <td style="display:none">${pep.ciudad.ciudadid}</td>
                                                <td>${pep.ciudad.nombre}</td> 
                                                <td>${pep.personaid.nombre}</td>                                        
                                                <td>${pep.personaid.apaterno}</td> 
                                                <td>${pep.personaid.amaterno}</td> 
                                                <td>${pep.personaid.curp}</td> 
                                                <td style="display:none"> ${pep.personaid.sexoid.sexoid}</td> 
                                                <td> ${pep.personaid.sexoid.descripcion}</td>                                          
                                                <td style="display:none"> ${pep.estatuspbapepid.estatuspbapepid}</td> 
                                                <td> ${pep.estatuspbapepid.descripcion}</td> 
                                                <td><button id="mostrarPep" onclick="mostrarPEP();" data-toggle="modal" data-target="#myModal" class="btn btn-warning">
                                                        <span class="glyphicon glyphicon-pencil"></span> </button></td>
                                            </tr>
                                        </c:forEach>
                                    </tbody>
                                </table>
                            </div>     <!-- Table-reponisve-->      
                            <input type="hidden" name="${_csrf.parameterName}"
                                   value="${_csrf.token}" />
                        </div><!--col-m-12 -->
                    </div><!-- row-->
                </div><!--class body -->
            </div><!-- panel-primary-->
        </div>  <!-- panel-group-->  
    </div><!--class container -->
</div>
<div class="container">
    <!-- Modal PEP -->
    <div class="modal fade" id="myModal" role="dialog"data-backdrop="static" 
         data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="border-color:#0000FF">
                <div class="modal-header" style="border-color:#0000FF; background: linear-gradient(to bottom, rgba(252,234,187,1) 0%, rgba(248,181,0,1) 51%, rgba(252,205,77,1) 64%, rgba(251,223,147,1) 100%);" >
                    <h3> Personas Politicamente expuestas</h3>
                </div>
                <div class="modal-body">
                    <!--Formilario para agregar pep -->
                    <form class="form-horizontal" id="frmPep">
                        <div class="form-group">
                            <label  class="control-label col-sm-2">Fecha Vigente:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-3">
                                <input type="date" class="form-control" id="fechPep" placeholder="Ingrese la fecha correspondiente">
                            </div>                            
                            <label  class="control-label col-sm-1">Cargo:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="cargoPep" class="form-control">
                                    <option value="0">Seleccione</option>
                                    <c:forEach items="${lCargoPolitico}" var="cargo">
                                        <option value="${cargo.cargoid}">${cargo.nombre} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>  
                            <label  class="control-label col-sm-1">Ciudad:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-3">
                                <!-- consulta catalogo -->
                                <select id="ciudadPep" class="form-control">
                                    <option value="0">Seleccione</option>
                                    <c:forEach items="${lCiudad}" var="ciudad">
                                        <option value="${ciudad.ciudadid}">${ciudad.nombre} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>  
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" >Nombre:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="nomPep" placeholder="Ingrese el nombre" onkeyup="mayuscula(this)">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2">Apellido Paterno:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apPatPep" placeholder="Ingrese el apellido paterno"onkeyup="mayuscula(this)">
                            </div>
                            <label class="control-label col-sm-2">Apellido Materno:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apMatPep" placeholder="Ingrese el apellido mmaterno"onkeyup="mayuscula(this)">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2" >CURP:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="curpPep" placeholder="Ingrese la curp"onkeyup="mayuscula(this)">
                            </div>
                            <label  class="control-label col-sm-1">Sexo:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="sexoPep" class="form-control">
                                    <option value="0">Seleccione sexo </option>
                                    <c:forEach items="${lSexo}" var="sexo">
                                        <option value="${sexo.sexoid}">${sexo.descripcion} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>   

                            <label  class="control-label col-sm-1">Estatus:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="estatusPep" class="form-control">
                                    <option value="0">Seleccione el Estatus</option>
                                    <c:forEach items="${lEstPba}" var="estatus">
                                        <option value="${estatus.estatuspbapepid}">${estatus.descripcion} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>                           
                        </div> 

                    </form> 
                </div><!--modal body-->
                <div class="modal-footer">
                    <div class="form-group">
                        <button id="guardarPep" type="button" class="btn btn-success" onclick="agregarPep();">Guardar</button>
                        <button id="actualizarPep" onclick="actualizarPep();" class="btn btn-primary">Actualizar</button>                        
                      <a id="pep" href="<c:url value='/pep'/>"> <button type="button" class="btn btn-default" onclick="limpiarForm('frmPep');">Cerrar</button></a>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- ventana modal-->                               
</div> <!-- panel-container-->                            
<!--FAMILIARES DE UNA PERSONA PEP-->
<div id="familiar" style="display: none;">
    <div class="container">   
        <div class="panel-group">
            <div class="panel panel-primary">
                <div class="panel-heading">Familiares de Personas Politicamente expuestas 
                    <label id="verPep" onclick="verTablaP();" style="float:right;cursor: pointer"><i class="fa fa-reply-all border-input"></i>Regresar</label>
                </div>
                <div id="contenidoPantalla" class="panel-body">        
                    <div class="row">
                        <div class="form-group">
                            <button id="AgregarF" class="btn btn-info" data-toggle="modal"  data-target="#myModal2"> 
                                <span class="glyphicon glyphicon-plus"></span>Agregar Familiar</button>
                            <input type="text" class="form-control pull-right" style="width:20%"
                                   id="searchF" onkeyup="busquedaTblF('tableFamilia', 'searchF');" placeholder="Busqueda"/>
                        </div>
                        <!--Familiar -->
                        <div  class="col-md-12">
                            <div class="table-responsive">
                                <table id="tableFamilia" class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th style="display: none">IDFAM</th>
                                            <th>No</th>
                                            <th style="display: none">IDPEP</th>
                                            <th>Persona PEP</th>                                        
                                            <th>Nombres</th>
                                            <th>Apellido Paterno</th>
                                            <th>Apellido Materno</th>  
                                            <th>CURP</th>
                                            <th style="display:none">Sexo ID</th>
                                            <th>Sexo</th>
                                            <th style="display:none">ParentescoId</th>
                                            <th>Parentesco</th>                                     
                                            <th>Acualizar</th>
                                            <th>Eliminar</th>
                                        </tr>
                                    <thead>
                                    <tbody id="bodytabla">
                                        <c:forEach items="${lFamiliar}" var="fam" varStatus="cont">
                                            <tr>
                                                <td id="idfam" style="display: none">${fam.familiarid}</td>   
                                                <td>${cont.count}</td>
                                                <td id="idpep" style="display: none">${fam.pepid.pepid}</td>
                                                <td>${fam.pepid.personaid.nombre} ${fam.pepid.personaid.apaterno} ${fam.pepid.personaid.amaterno}</td>
                                                <td>${fam.personaid.nombre}</td> 
                                                <td>${fam.personaid.apaterno}</td> 
                                                <td>${fam.personaid.amaterno}</td> 
                                                <td>${fam.personaid.curp}</td> 
                                                <td style="display:none"> ${fam.personaid.sexoid.sexoid}</td> 
                                                <td> ${fam.personaid.sexoid.descripcion}</td> 
                                                <td style="display:none"> ${fam.parentescoid.parentescoid}</td> 
                                                <td > ${fam.parentescoid.descripcion}</td> 

                                                <td><button  type="button" id="editarF"  class="btn btn-warning"onclick="mostratFam();" data-toggle="modal" data-target="#myModal2" >
                                                        <span class="glyphicon glyphicon-pencil"></span></button>
                                                </td>
                                                <td><button type="button" id="eliminar" class="btn btn-danger" onclick="eliminarFamiliar();">
                                                            <span class="glyphicon glyphicon-remove"></span> </button></td>

                                            </tr>
                                        </c:forEach>
                                    </tbody>
                                </table>

                            </div>     <!-- Table-->      
                            <input type="hidden" name="${_csrf.parameterName}"
                                   value="${_csrf.token}" />
                        </div><!--col-m-12Familiar -->                 
                    </div><!-- row-->
                </div><!--class body -->
            </div><!-- panel-primary-->
        </div>  <!-- panel-group-->  
    </div><!--class container -->
</div>
<div class="container">
    <!-- Modal -->
    <div class="modal fade" id="myModal2" role="dialog"data-backdrop="static" 
         data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="border-color:#0000FF">
                <div class="modal-header" style="border-color:#0000FF; background: linear-gradient(to bottom, rgba(252,234,187,1) 0%, rgba(248,181,0,1) 51%, rgba(252,205,77,1) 64%, rgba(251,223,147,1) 100%);" >
                    <h4>Familiares</h4>
                </div>
                <div class="modal-body">
                    <!--Formilario para agregar usuario -->
                    <form class="form-horizontal" id="frmFam">
                        <div class="form-group">


                            <label  class="control-label col-sm-2">PEP:<b style="color: red" title="Campo Requerido">*</b> </label>
                            <div class="col-sm-4">
                                <!-- consulta catalogo -->
                                <select id="idPep" class="form-control">
                                    <option value="0">Seleccione PEP</option>
                                    <c:forEach items="${lPep}" var="pep">
                                        <option value="${pep.pepid}">${pep.personaid.nombre} ${pep.personaid.apaterno} ${pep.personaid.amaterno} </option>
                                    </c:forEach>
                                </select>  
                            </div> 
                            <label  class="control-label col-sm-2">Parentesco:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="parentescoFam" class="form-control">
                                    <option value="0">Seleccione</option>
                                    <c:forEach items="${lPare}" var="parentesco">
                                        <option value="${parentesco.parentescoid}">${parentesco.descripcion} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>

                        </div>
                        <!--Familiar -->

                        <div> <h4> Familiar Politicamente expuestas</h4></div>
                        <div class="form-group">

                            <label class="control-label col-sm-2">Nombre:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="nomFam" onchange="mayuscula(this)">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2">Apellido Paterno:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apPatFam" onchange="mayuscula(this)" >
                            </div>
                            <label class="control-label col-sm-2">Apellido Materno:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="apMatFam" onchange="mayuscula(this)">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-2">CURP:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="curpFam" onchange="mayuscula(this)">
                            </div>
                            <label  class="control-label col-sm-1">Sexo:<b style="color: red" title="Campo Requerido">*</b></label>
                            <div class="col-sm-2">
                                <!-- consulta catalogo -->
                                <select id="sexoFam" class="form-control">
                                    <option value="0">Seleccione sexo </option>
                                    <c:forEach items="${lSexo}" var="sexo">
                                        <option value="${sexo.sexoid}">${sexo.descripcion} </option>
                                    </c:forEach>
                                </select>                                            
                            </div>

                        </div>
                    </form> 
                </div><!--modal body-->
                <div class="modal-footer">
                    <div class="form-group">
                        <button id="GuardarF" type="button" class="btn btn-success" onclick="agregarFamiliar();">Guardar</button>
                        <button id="ActualizarF" onclick="actualizarFamiliar();" class="btn btn-primary" >Actualizar</button>
                        <button type="button" class="btn btn-default"  id="cerrarFam" onclick="limpiarForm('frmFam');" data-dismiss="modal">Cerrar</button>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- ventana modal-->                               
</div> <!-- panel-container-->
</body>
</html>
