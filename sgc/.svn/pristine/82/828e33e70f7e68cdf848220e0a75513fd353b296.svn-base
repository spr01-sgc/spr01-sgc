package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.PaisDao;
import com.soma.transmisor.model.Pais;
import com.soma.transmisor.service.PaisService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JASMIN-SOMA
 */
@Service("paisService")
@Transactional
public class PaisServiceImpl implements PaisService{
@Autowired
PaisDao paisDao;
    @Override
    public Pais buscaId(int paisId) {
   return paisDao.buscaId(paisId);
    }

    @Override
    public List<Pais> showPais() {
    return paisDao.showPais();
    }
    
}
