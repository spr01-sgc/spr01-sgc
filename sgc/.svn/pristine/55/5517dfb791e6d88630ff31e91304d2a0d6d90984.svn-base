package com.soma.transmisor.service.impl;

import com.soma.transmisor.dao.CargoPoliticoDao;
import com.soma.transmisor.model.CargoPolitico;
import com.soma.transmisor.service.CargoPoliticoService;
import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
 *
 * @author JASMIN-SOMA
 */
@Service("cargoPoliticoService")
@Transactional
public class CargoPoliticoServiceImpl implements CargoPoliticoService{
    @Autowired 
    CargoPoliticoDao cargoPoliticoDao;

    @Override
    public CargoPolitico buscaId(int cargoid) {
    return cargoPoliticoDao.buscaId(cargoid);
    }

    @Override
    public List<CargoPolitico> showCargoP() {
    return cargoPoliticoDao.showCargoP();
    }
    
}
